{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "@components": "./src/components",
              "@Redux": "./src/redux"
            }
          }
        ],
        [
          "styled-components",
          {
            "ssr": true
          }
        ]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "@components": "./src/components",
              "@Redux": "./src/redux"
            }
          }
        ],
        [
          "styled-components",
          {
            "ssr": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "@components": "./src/components",
              "@Redux": "./src/redux"
            }
          }
        ]
      ]
    }
  }
}
